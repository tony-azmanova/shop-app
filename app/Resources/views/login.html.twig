{# empty Twig template #}
{% extends 'base.html.twig' %}
{% block body %}

    <h1>Login!</h1>
    {% if error %}
        <div>{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}

    {{ form_start(form) }}
        {{ form_row(form._username) }}
        {{ form_row(form._password) }}
        <button class="btn btn-primary" type="submit">Login</button>
    {{ form_end(form) }}
  
{% endblock %}
